{
  "description": "Removes *innen|:innen from german webpages",
  "manifest_version": 2,
  "name": "Genderless",
  "version": "1.0",
  "homepage_url": "https://github.com/zach-all/firefox-genderless",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.de/*"],
      "js": ["genderless.js"]
    }
  ]
}
